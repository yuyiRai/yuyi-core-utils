{
  "name": "yuyi-core-utils",
  "version": "0.1.52",
  "description": "",
  "keywords": [
    "mobx",
    "lodash",
    "typescript"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es5.js",
  "typings": "types",
  "files": [
    "dist"
  ],
  "homepage": "https://yuyirai.github.io/yuyi-core-utils/",
  "author": "yuyiRai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyiRai/yuyi-core-utils.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "docs": "yuyidocs ../..",
    "docs:full": "yuyidocs ../.. -i",
    "docs:watch": "yuyidocs ../.. -w",
    "predeploy-docs": "yarn run docs:full",
    "deploy-docs": "gh-pages -d docs",
    "lint": "tslint --project tsconfig.json -t codeFrame 'src/**/*.ts' 'test/**/*.ts'",
    "dev": "ttsc --project tsconfig.json --watch",
    "prebuild": "rimraf dist",
    "prebuild2": "rimraf dist/**/*.js && rimraf dist/**/*.map",
    "build": "ttsc --project tsconfig.json && rollup -c rollup.config.ts && yarn run deploy-docs",
    "start": "rollup -c rollup.config.ts -w",
    "start:all": "rollup -c rollup.config.ts -w && yarn run docs:watch",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "semantic-release-prepare": "ts-node tools/semantic-release-prepare",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "mobx": "4.9.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/gulp": "^4.0.6",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.4",
    "colors": "^1.3.2",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "gulp": "^4.0.2",
    "gulp-shell": "^0.7.0",
    "gulp-typedoc": "^2.2.2",
    "husky": "^2.3.0",
    "immer": "^3.1.2",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "lint-staged": "^8.0.0",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.1",
    "lodash.camelcase": "^4.3.0",
    "mobx": "4.9.4",
    "mobx-utils": "^5.4.0",
    "prettier": "^1.14.3",
    "prompt": "^1.0.0",
    "replace-in-file": "^4.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.13.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.21.1",
    "rxjs": "^6.5.2",
    "semantic-release": "^15.9.16",
    "shelljs": "^0.8.3",
    "travis-deploy-once": "^5.0.9",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.14.2",
    "typedoc-default-themes": "^0.5.0",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-external-module-map": "^1.0.0",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-folder-modules": "^0.1.8",
    "typedoc-plugin-localization": "^1.4.1",
    "typedoc-plugin-no-inherit": "^1.1.8",
    "typedoc-plugin-toc-group": "^0.0.5",
    "typescript": "3.5.1"
  }
}
